{
  "environment": "production",
  "debug_mode": false,
  "api_version": "v1.0.2",
  "security_level": "maximum",
  "encryption_enabled": true,
  "admin_access": {
    "require_2fa": true,
    "session_timeout": 3600,
    "max_login_attempts": 3
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_minute": 100,
    "burst_limit": 200
  },
  "logging": {
    "level": "info",
    "security_events": true,
    "audit_trail": true,
    "retention_days": 90
  },
  "features": {
    "real_time_scanning": true,
    "ml_detection": true,
    "behavioral_analysis": true,
    "cloud_intelligence": true,
    "memory_scanning": true,
    "network_monitoring": true
  },
  "database": {
    "type": "encrypted",
    "backup_enabled": true,
    "compression": true
  }
}
